<div class="header3">
   <h3><%= link_to "<", group_lists_path(@group), class: "rounded-circle px-2 bg-primary" %></h3>
   <h3 class="uppercase">Create a new list</h3>
</div>
<div class="container">
  <%= simple_form_for [@group, @list] do |f| %>
    <%= f.input :name %>
    <%= f.input :photo %>
    <%= f.submit "Create a new List", class: "button" %>
  <% end %>
</div>
